<script type="text/javascript">

	var ymd = "null";
	$(function() {
		calendar.makeCalendarHTML();
		calendar.elementId = "schedule";
		calendar.onClickButton("btnCalendar");
		if (ymd.length >= 8) {
			calendar.rYear = ymd.substring(0, 4);
			calendar.rMonth = ymd.substring(4, 6);
			calendar.rDay = ymd.substring(6, 8);
		}
		calendar.selCalendarDatePrev = function() {
			var date = calendar.rYear
					+ ""
					+ (((calendar.rMonth + "").length < 2) ? "0"
							+ calendar.rMonth : calendar.rMonth)
					+ (((calendar.rDay + "").length < 2) ? "0" + calendar.rDay
							: calendar.rDay);
			location.href = "http://w3.sbs.co.kr/schedule/main.do?ymd=" + date;
		};
		var today = new Date();
		var currHour = "08";
		var idx = 0;
		if (Number(today.getFullYear()) == Number(calendar.rYear)
				&& (Number(today.getMonth()) + 1) == Number(calendar.rMonth)
				&& Number(today.getDate()) == Number(calendar.rDay)) {
			showBarThisTime(currHour);
			if (Number(currHour) < 2)
				idx = 0;
			else if (Number(currHour) >= 2)
				idx = Number(currHour) - 1;
			else if (Number(currHour) > 21)
				idx = 21;
		}
		slideSheet(idx);
	});
	function showBarThisTime(idx) {
		var $timeList = $(".stt_table thead th[colspan=12]");
		$timeList.eq(idx).addClass("stt_today").children(".stt_time_w").append(
				"<span class=\"stt_nowtime\">방송시각</span>");
	}
	function showDetailLayer(elId, obj) {
		$('td').each(function() {
			$(this).removeAttr('style');
		});
		if (obj != undefined) {
			$(obj).parent().attr('style', 'position:relative;z-index:10;');
		}
		var $detailList = $('.stt_pro_detail');
		var $detailItem;
		$detailList.each(function(idx) {
			$detailItem = $detailList.get(idx);
			if ($detailItem.id != elId)
				$($detailItem).hide();
		});
		$(obj).children(".stt_pro_detail").css('display', 'block');
	}
	function layerDisplayNone(obj) {
		if (obj != undefined) {
			$(obj).css('display', 'none');
			$(obj).parent().parent().removeAttr('style');
		}
	}
	function closeLayer(obj) {
		if (obj != undefined) {
			$(obj).parent().css('display', 'none');
			$(obj).parent().parent().parent().removeAttr('style');
		}
	}
	var slidePositionIndex = 0;
	var COL_WIDTH = 264;
	function slideSheet1step(point) {
		debugger;
		if (point == 'R' && slidePositionIndex < 15){
			slidePositionIndex = slidePositionIndex + 6;
		}else if (point == 'L' && slidePositionIndex > 0){
			slidePositionIndex = slidePositionIndex - 6;
		}else{
			slidePositionIndex = 0;
		}
		slideSheet(slidePositionIndex);
	}
	function slideSheet(index) {
		debugger;
		var slideWidth = 0;
		if (index != null && index >= 0 && index <= 15){
			slidePositionIndex = index;
		}
		slideWidth = -(slidePositionIndex * COL_WIDTH);
		$(".stt_table").animate({
			"left" : slideWidth + "px"
		});
		if (slidePositionIndex < 12) {
			$(".btn_stt_pm").removeClass("current");
			$(".btn_stt_am").addClass("current");
		} else {
			$(".btn_stt_am").removeClass("current");
			$(".btn_stt_pm").addClass("current");
		}
	}
</script>

